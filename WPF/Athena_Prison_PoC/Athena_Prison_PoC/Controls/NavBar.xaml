<UserControl x:Class="Athena_Prison_PoC.Controls.NavBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Athena_Prison_PoC.Controls"
             xmlns:ViewModels="clr-namespace:Athena_Prison_PoC.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800">

    <Grid >
        <ListBox Style="{StaticResource NavBarListBoxStyle}" ItemsSource="{Binding Pages}" SelectedItem="{Binding CurrentPage}"/>
    </Grid>
</UserControl>
